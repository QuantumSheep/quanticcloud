{
  "name": "quanticcloud",
  "version": "1.0.0",
  "description": "Quantic Cloud",
  "scripts": {
    "install": "cd client && npm install && cd ../server && npm install && cd ..",
    "build": "cd client && ng build && cd ..",
    "build:watch": "cd client && ng build --watch && cd ..",
    "start": "npx pm2 start process.json",
    "stop": "npx pm2 stop process.json",
    "restart": "npx pm2 restart process.json",
    "delete": "npx pm2 delete process.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuantumSheep/quanticcloud.git"
  },
  "author": "QuantumSheep",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QuantumSheep/quanticcloud/issues"
  },
  "homepage": "https://github.com/QuantumSheep/quanticcloud#readme",
  "dependencies": {
    "pm2": "^2.10.2"
  }
}
